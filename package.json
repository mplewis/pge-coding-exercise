{
  "name": "pge-coding-exercise",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "build": "tsc -p .",
    "clean": "rm -rf dist",
    "dev": "nodemon --watch src --ignore '**/dist/**' --ext ts --exec \"pnpm build && node\" dist/serve.js",
    "docker-build": "docker build -t pge-coding-exercise .",
    "docker-run": "docker run --init -p 3000:3000 -it pge-coding-exercise",
    "test": "vitest"
  },
  "dependencies": {
    "@hapi/hapi": "^21.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.134",
    "@types/node": "^20.11.22",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vitest": "^1.3.1"
  }
}
